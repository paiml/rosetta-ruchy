// Selection Sort 100% Coverage - v1.27.10 Compatible

fun selection_sort_demo(size: i32) -> i32 {
    if size <= 0 {
        return 0;
    }
    
    let mut swaps: i32 = 0;
    let mut i: i32 = 0;
    
    while i < size - 1 {
        let mut min_idx: i32 = i;
        let mut j: i32 = i + 1;
        
        while j < size {
            // Simulate comparison
            if j < min_idx {
                min_idx = j;
            }
            j = j + 1;
        }
        
        if min_idx != i {
            swaps = swaps + 1;
        }
        
        i = i + 1;
    }
    
    return swaps;
}

