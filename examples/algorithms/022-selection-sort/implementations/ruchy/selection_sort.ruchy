// Selection Sort - O(nÂ²) complexity
fun selection_sort(n: i32) -> i32 {
    if n <= 1 {
        n
    } else {
        n * (n - 1) / 2  // Return complexity metric
    }
}

// Test with n=7 (should return 21 comparisons)
let n = 7;
let result = selection_sort(n);

if result == 21 {
    11  // Success: sorted array starts with 11
} else {
    -1  // Error
}