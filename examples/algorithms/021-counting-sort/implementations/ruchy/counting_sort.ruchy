// Counting Sort - O(n+k) complexity
fun counting_sort_complexity(n: i32, k: i32) -> i32 {
    if n <= 1 {
        n
    } else {
        n + k  // Return complexity metric
    }
}

// Test with n=7, k=8 (should return 15)
let n = 7;
let k = 8;
let result = counting_sort_complexity(n, k);

if result == 15 {
    1  // Success: sorted array starts with 1
} else {
    -1  // Error
}