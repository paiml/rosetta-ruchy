// Check if city is in subset (simplified bitmask)
fun city_in_subset(subset: i32, city: i32) -> bool {
    if city >= 0 && city < 10 {
        let power_of_2 = if city == 0 { 1 } 
                        else if city == 1 { 2 }
                        else if city == 2 { 4 }
                        else if city == 3 { 8 }
                        else if city == 4 { 16 }
                        else if city == 5 { 32 }
                        else if city == 6 { 64 }
                        else if city == 7 { 128 }
                        else if city == 8 { 256 }
                        else { 512 };
        let remainder = subset % (power_of_2 * 2);
        return remainder >= power_of_2;
    }
    false
}

