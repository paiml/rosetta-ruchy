fn binary_search(arr: &[i32], target: i32) -> i32 {
    let mut left = 0;
    let mut right = arr.len() as i32;
    
    while left < right {
        let mid = left + (right - left) / 2;
        let mid_val = arr[mid as usize];
        
        if mid_val == target {
            return mid;
        } else if mid_val < target {
            left = mid + 1;
        } else {
            right = mid;
        }
    }
    
    -1
}

fn main() {
    let data = vec![1, 3, 5, 7, 9];
    println!("Ruchy v1.5.0 Binary Search Test");
    
    let result = binary_search(&data, 5);
    if result >= 0 {
        println!("Found 5 at index {}", result);
    } else {
        println!("5 not found");
    }
}